{
	"env": {
        "browser": true,
        "es2022": true,
        "node": true
	},
	"extends": [
		"plugin:react/recommended",
		"eslint:recommended"
	],
	"overrides": [],
	"parserOptions": {
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"indent": [
			"warn",
			"tab"
		],
		"no-unused-vars": "warn",
		"semi": "warn",
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off"
	}
}